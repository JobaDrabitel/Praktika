<Window x:Class="Praktika.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Praktika"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800" ResizeMode="NoResize"  WindowStartupLocation="CenterScreen">
    <WrapPanel>
        <Button x:Name="ClientListButton" Click="ClientListButton_Click">Client List</Button>
        <Button x:Name="AgentListButton" Click="AgentListButton_Click">Agent List</Button>
        <Button x:Name="RealtyListButton" Click="RealtyListButton_Click">Realty List</Button> 
        <Button x:Name="DemandListButton" Click="DemandListButton_Click">Demand List</Button>
        <Button x:Name="SupplyListButton" Click="SupplyListButton_Click">Supply List</Button>
        <Button x:Name="DealListButton" Click="DealListButton_Click">Deal List</Button>
    </WrapPanel>
</Window>
